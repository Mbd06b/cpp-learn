#ifndef UCSTRING_H_
#define UCSTRING_H_



class UCString : public String{

public:
					UCString();
					~UCString();

private:
		size_t	numChars; //size_t aka unsigned long (positive only)
		size_t numSlots;
		char * pData;

		//variables for concatenation
		size_t numTempChars;
		char * pTempData;

};

#endif /* UCSTRING_H_ */


UCString::UCString(){

};

UCString::~UCString(){
};
