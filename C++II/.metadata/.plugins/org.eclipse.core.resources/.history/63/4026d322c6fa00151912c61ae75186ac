#include "Address.h"


Address::Address(){
		sStreet = "";
		sCity = "";
	    sState = "";
	    sZip = "";
	}

Address::Address(Address & toCopy){
	sStreet = toCopy.sStreet;
	sCity = toCopy.sCity;
	sState = toCopy.sState;
	sZip = toCopy.sZip;
}

Address::~Address() {
}

