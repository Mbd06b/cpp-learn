#include <iostream>
#include "Name.h"
#include "myStringclass.h"
using namespace std;

int main() {

	Name A1;

	A1.setName("Matthew", first);
	A1.setName("Blake", middle);
	A1.setName("Dowell", last);

	for(int i = first; i < enumEnd; i++){
	cout << A1.getName((whichName)i) << " ";
	}


	return 0;
}
