#include <iostream>
using namespace std;

int main() {
	cout << "Enter two whole numbers; ";
	cin >> L1 >> L2;
		// <compares two values to see if first is less than the second
	if (L1 > L2) // what is inside the parenteses is checked for true false
			cout << L1 << "is less than " << L2 << endl;

	return 0;
}
