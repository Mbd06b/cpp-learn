#include "Array.h"
#include <iostream>
using namespace std;


#include <iostream>
using namespace std;

int main (){

Array <int, -3, 6>  testObject;

Array <int, -3, 6>  secondObj;


try{

testObject.setAt(-3,10);
testObject.setAt(-2,10);
testObject.setAt(-4, 15);


}catch (int e){

	cout << "Exception " << e <<  " thrown, Index Out of Bounds!" << endl;

};



 cout << "/n Program Ended";
	return 0;
}
















