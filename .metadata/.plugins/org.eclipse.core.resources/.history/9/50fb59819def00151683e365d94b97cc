#ifndef MYCLASS_H_
#define MYCLASS_H_

class Test1
{
	public:
			Test1 () {A = -5;}

			int	GetB () const {return B;} //convinent subscript style

	private:
				   int A;
			static int B; //one copy of this is shared among all the instances of this class.

};






#endif /* MYCLASS_H_ */
